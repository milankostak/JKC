{var $title = "Zveřejnit článek: ".$article->title}

{block content}

<div class="row admin-article">
<div class="col-sm-3">
	{include article_menu.latte}
</div>
<div class="col-sm-9">
	<h2>Zveřejnit článek: {$article->title}</h2>

	{include one_article_menu.latte}
	{control immediatePublishForm}
	<br>
	{form publishForm}
		<p><strong>{label datetime /}</strong></p>
		<p>{input datetime}</p>
		<p>{input save}</p>
	{/form publishForm}
</div>
</div>
